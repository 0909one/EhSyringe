{
  "manifest_version": 2,
  "name": "EhSyringeLite",
  "description": "EhSyringeLite",
  "author": "xioxin",
  "version": "1.0",
  "icons": {
    "32": "assets/logo32.png",
    "16": "assets/logo16.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "omnibox": {
    "keyword" : "ex"
   },
  "background": {
    "scripts": ["script/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://exhentai.org/*", "*://e-hentai.org/*", "*://*.exhentai.org/*", "*://*.e-hentai.org/*"],
      "css": [ ],
      "js": [ "script/syringe.js" ],
      "run_at": "document_start"
    },
    {
      "matches": ["*://exhentai.org/*", "*://e-hentai.org/*", "*://*.exhentai.org/*", "*://*.e-hentai.org/*"],
      "css": [ ],
      "js": ["script/tag-tip.js" ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://exhentai.org/*",
    "*://e-hentai.org/*",
    "*://*.exhentai.org/*",
    "*://*.e-hentai.org/*",
    "*://github.com/*","*://*.amazonaws.com/*",
    "activeTab",
    "notifications",
    "storage",
    "contextMenus",
    "omnibox"
  ]
}
