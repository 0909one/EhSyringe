{
  "manifest_version": 2,
  "name": "EhSyringe",
  "description": "E站注射器，将中文翻译注入到E站体内。",
  "author": "xioxin",
  "version": "1.2.1",
  "icons": {
    "32": "assets/logo32.png",
    "16": "assets/logo16.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "omnibox": {
    "keyword": "ex"
  },
  "background": {
    "scripts": [
      "script/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "assets/logo32.png",
      "16": "assets/logo16.png",
      "48": "assets/logo48.png",
      "128": "assets/logo128.png"
    },
    "default_title": "EhSyringe",
    "default_popup": "template/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://exhentai.org/*",
        "*://e-hentai.org/*",
        "*://*.exhentai.org/*",
        "*://*.e-hentai.org/*"
      ],
      "js": [
        "script/main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://exhentai.org/*",
    "*://e-hentai.org/*",
    "*://*.exhentai.org/*",
    "*://*.e-hentai.org/*",
    "https://github.com/EhTagTranslation/*",
    "https://*.amazonaws.com/*",
    "activeTab",
    "notifications",
    "storage",
    "contextMenus"
  ]
}
